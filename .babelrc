{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "styled-jsx-plugin-sass"
          ]
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "lib": "./lib",
          "pages": "./pages",
          "layouts": "./layouts/",
          "variables": "./components/styles/variables.js"
        }
      }
    ]
  ]
}
